
	<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width,initial-scale=1.0">
		<title>MessUp</title>
		<link href="assets/stylesheets/bootstrap.min.css" rel="stylesheet">
	</head>
	<body>
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
  		<div class="nav navbar-header">
    		<a href="/devise/home/contact" class="navbar-brand">Contact</a>
  		</div>
  		<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    		<ul class="nav navbar-nav">
      		<li class="dropdown">
        		<a href="#" class="dropdown-toggle" data-toggle="dropdown">English <b class="caret"></b></a>
        		<ul class="dropdown-menu">
          	<li><a href="#">German</a></li>
          	<li><a href="#">Spanish</a></li>
          	<li><a href="#">Hindi</a></li>
          	<li><a href="#">Chinese</a></li>
        		</ul>
      		</li>
    		</ul>
    		<ul class="nav navbar-nav pull-right">
      		<!--<%= link_to("New member registration",{:action => 'sign_up'}, :class => 'action sign_up') %>-->
      		<li>
      			<%= link_to("LogOut",{:action => 'sign_in'}, :class => 'action sign_in') %>
      		</li>
    		</ul>
  		</div><!-- /.navbar-collapse -->
		</nav>
		<div class="container" style = "margin-top: 50px">
			<div class="well">
				<h1>
		  	Welcome to Our website.
				</h1>
				<div >
					<center>
						<table class="table table-striped table-hover ">	
							<th>
								<tr style = "background-color: #def">
								<td>ID</td>
								<td>First Name</td>
								<td>Last Name</td>
								<td>email</td>
								<td>institution</td>
								<td>password</td>
								<td></td>
								<td></td>
								<td></td>
								</tr>
						  </th>
							
							<% @users.each do |t|%>
								<tr>
									<td><%= t.id%></td>
									<td><%= t.firstname%></td>
									<td><%= t.lastname%></td>
									<td><%= t.email%></td>
									<td><%= t.institution%></td>
									<td><%= t.password%></td>
									<td><%= link_to("view",{:action => 'show', :id => t.id}, :class => 'action show')%></td>
									<td><%= link_to("edit",{:action => 'edit', :id => t.id}, :class => 'action edit')%></td>
									<td><%= link_to("delete",{:action => 'delete', :id => t.id}, :class => 'action delete')%></td>
								</tr>
							<%end%>
						</table>
					</center>
				</div>
			</div>
		</div>      
	</body>
	</html>
